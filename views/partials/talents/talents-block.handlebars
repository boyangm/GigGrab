   <div class="card">
        <div class="card-image">
            {{#if dataValues.image}}
          <img src={{dataValues.image}} >
          {{else}}
          <img src="https://image.shutterstock.com/image-vector/male-default-placeholder-avatar-profile-260nw-387516193.jpg">
        {{/if}}
          <span class="card-title">Card Title</span>
          <a class="btn-floating halfway-fab waves-effect waves-light red"><i class="material-icons">add</i></a>
        </div>
        <div class="card-content">
            <h6>{{dataValues.name}}</h6>
          <p>{{dataValues.instrument}}</p>
         <div class="stats">
        <i class="material-icons star">star</i>
        <h5>{{dataValues.rating}}</h5>
        </div> 
        </div>
      </div>